    "version",
    "output",
    "parameters"
  ],
  "properties": {
    "version": {
      "$id": "#/properties/XYZ/properties/version",
      "type": "number",
      "title": "The Version Schema",
      "default": 0.0,
      "examples": [
        2.1
      ],
      "enum": [
        1.0,
        1.1
      ]
    },
    "output": {
      "$id": "#/properties/XYZ/properties/output",
      "type": "string",
      "title": "The Output Schema",
      "default": "",
      "examples": [
        "streamflow"
      ],
      "enum": [
        "streamflow"
      ]
    },
    "parameters": {
      "$id": "#/properties/NWM/properties/parameters",
      "type": "object",
      "ref": "file:nwm.model.parameter.schema.json"
    }
  }
}
